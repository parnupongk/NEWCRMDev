@model NEWCRM.Models.ListRepCaseModel

<div class="row">
    <div class="col-lg2" style="padding: 0 30px 10px 30px;"><a href="@Url.Action("excelCallHour", "RepCase")" class="btn ink-reaction btn-success"><i class="fa fa-file-excel-o"></i>&nbsp;Export To Excel</a></div>
</div>
<div class="row">
    <div class="col-lg-12" style="padding: 0 30px 10px 30px;">
        <div class="table-responsive">
            <table width="100%">
                <thead>
                    <tr class="gradeX">
                        <th colspan="2">
                            <h3>ETDA Call Center</h3>
                            <h4>Call Performance Report By Hour (Daily)</h4>
                        </th>
                        <th colspan="2"></th>
                        <th>
                            <div class="form-group no-padding pull-right">
                                <img width="190px" height="100px" src="http://parnupongk.azurewebsites.net/images/logo.png" />
                            </div>
                        </th>
                    </tr>
                </thead>
            </table>
            <table id="datatable1" class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th>Hour</th>
                        <th>Incoming Call</th>
                        <th>Transfer to Agent</th>
                        <th>Answer Call</th>
                        <th>Abandoned</th>
                        <th>% Abandoned</th>
                        <th>Average talk time</th>
                        <th>Total talk time</th>
                    </tr>
                </thead>
               
            </table>
        </div>
    </div>
</div>