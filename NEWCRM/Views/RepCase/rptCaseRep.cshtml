@model NEWCRM.Models.ListRepCaseModel

<div class="row">
    <div class="col-lg2" style="padding: 0 30px 10px 30px;"><a href="@Url.Action("excelCaseRep", "RepCase", new { startDate = @ViewBag.startDate, endDate = @ViewBag.endDate, caseIDLevel1 = @ViewBag.caseIDLevel1, caseIDLevel2 = @ViewBag.caseIDLevel2, caseIDLevel3 = @ViewBag.caseIDLevel3, caseIDLevel4 = @ViewBag.caseIDLevel4 })"  class="btn ink-reaction btn-success"><i class="fa fa-file-excel-o"></i>&nbsp;Export To Excel</a></div>
</div>
<div class="row">
    <div class="col-lg-12" style="padding: 0 30px 10px 30px;">
        <div class="table-responsive">
            <table width="100%">
                <thead>
                    <tr class="gradeX">
                        <th colspan="2">
                            <h3>ETDA Call Center</h3>
                            <h4>Case Detail Report</h4>
                        </th>
                        <th colspan="2"></th>
                        <th>
                            <div class="form-group no-padding pull-right">
                                <img width="190px" height="100px" src="http://parnupongk.azurewebsites.net/images/logo.png" />
                            </div>
                        </th>
                    </tr>
                </thead>
            </table>
            <table id="datatable1" class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th>No.</th>
                        <th>Case ID</th>
                        <th>Created Date</th>
                        <th>Channel</th>
                        <th>Contact Name</th>
                        <th>Case Type</th>
                        <th>Phone Number</th>
                        <th>Detail</th>
                        <th>Status</th>
                        <th>Created By</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.list_repcase)
                    {
                        <tr class="gradeX">
                            <td></td>
                            <td>@item.casIDName</td>
                            <td>@item.casCreatedOn</td>
                            <td>@item.chnID</td>
                            <td>@item.casCreatedByName</td>
                            <td>@item.casSummary</td>
                            <th></th>
                            <td>@item.casdetail</td>
                            <th>@item.casstatusReason</th>
                            <td>@item.casOwnerByName</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>