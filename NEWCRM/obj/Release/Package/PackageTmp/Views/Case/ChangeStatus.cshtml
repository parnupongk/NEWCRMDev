@{
    ViewBag.Title = "ChangeStatus";
    Layout = null;
}

@using NEWCRM.Models
@model CaseModels
<div class="form-group">
    <select id="ddlStatus" class="form-control">
        @foreach (var Item in Model.CaseStatus)
        { 
         <option value="@Item.cssID">@Item.cssName</option>
        }
